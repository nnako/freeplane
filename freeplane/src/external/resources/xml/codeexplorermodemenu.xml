<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<FreeplaneUIEntries>
	<Entry name="main_toolbar" builder="toolbar,ignore">
		<Entry usedBy="EDITOR">
			<Entry name="SaveAction" row="1"/>
			<Entry name="code.PurgeAction" row="2"/>
		</Entry>
		<Entry builder="separator" />
		<Entry builder="main_toolbar_zoom" name="zoom" row="1" width = "4"/>
		<Entry name="PrintAction" row="2"/>
		<Entry name="CloseAction" row="2"/>
		<Entry row="2" usedBy="EDITOR">
			<Entry name="UndoAction" />
			<Entry name="RedoAction" />
		</Entry>
		<Entry builder="separator" />
		<Entry row="1">
			<Entry name="JumpInAction"/>
			<Entry name="UnfoldOneLevelAction" />
			<Entry name="UnfoldAllAction" />
		</Entry>
		<Entry row="2">
			<Entry name="JumpOutAction" />
			<Entry name="FoldOneLevelAction" />
			<Entry name="FoldAllAction" />
		</Entry>
		<Entry builder="separator" />
		<Entry name="SetBooleanPropertyAction.code_showOutsideDependencies"  builder = "bigIcon"/>
		<Entry builder="separator" />
		<Entry name="code.FilterClassesByDependencies" builder = "bigIcon"/>
		<Entry builder="separator" />
		<Entry row="1">
			<Entry name="code.ShowDependingNodesAction.INHERITING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INHERITED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.CONNECTED.NO_RECURSION" />
		</Entry>
		<Entry row="2">
			<Entry name="code.ShowDependingNodesAction.INHERITING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INHERITED.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.CONNECTED.MAXIMUM_RECURSION" />
		</Entry>
		<Entry builder="separator" />
		<Entry row="1">
			<Entry name="code.FilterCyclesAction" />
		</Entry>
		<Entry row="2">
			<Entry name="code.SelectCyclesAction" />
		</Entry>
		<Entry builder="separator" />
		<Entry name="code.ShowAllClassesAction" builder = "bigIcon"/>
	</Entry>
	<Entry name="main_menu" builder="main_menu">
		<Entry name="file">
			<Entry name="code.NewCodeMapAction" accelerator="control N"/>
			<Entry name="SaveAction" accelerator="control S" usedBy = "EDITOR" />
			<Entry name="CloseAction" accelerator="control W"/>
			<Entry name="lastOpenedMaps">
				<Entry builder="lastOpenedMaps" />
			</Entry>
			<Entry name="ExportAction" />
            <Entry name="print">
    			<Entry name="PageAction" />
    			<Entry name="PrintPreviewAction" />
    			<Entry name="PrintAction" accelerator="control P" />
            </Entry>
			<Entry builder="separator" />
			<Entry name="FilePropertiesAction" />
			<Entry builder="separator" />
			<Entry accelerator="control W" name="CloseAction" />
			<Entry accelerator="control Q" name="QuitAction" builder="MB_QuitAction" />
		</Entry>
		<Entry name="code.projectmenu">
			<Entry name="code.AddConfigurationAction" />
			<Entry name="code.DeleteConfigurationsAction" />
			<Entry builder="separator" />
			<Entry name="code.AddLocationsAction" />
			<Entry name="code.AddFoldersRecursivelyAction" />
			<Entry name="code.DeleteLocationsAction" />
		</Entry>
		<Entry name="code.exploremenu">
			<Entry name="code.RunAnalysisAction" accelerator="control R"/>
			<Entry builder="separator" />
			<Entry name="SetBooleanPropertyAction.code_showOutsideDependencies" />
			<Entry builder="separator" />
			<Entry name="code.FilterClassesByDependencies" />
			<Entry name="code.FilterClassesBySelectedDependencies" />
			<Entry builder="separator" />
			<Entry name="code.ShowDependingNodesAction.INHERITING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INHERITED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.CONNECTED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INHERITING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INHERITED.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.CONNECTED.MAXIMUM_RECURSION" />
			<Entry builder="separator" />
			<Entry name="code.FilterCyclesAction" />
			<Entry name="code.SelectCyclesAction" />
			<Entry builder="separator" />
			<Entry name="code.ShowAllClassesAction" />
		</Entry>
		<Entry name="edit">
			<Entry builder="radio_button_group">
				<Entry name="SetStringPropertyAction.view_mode.false" />
				<Entry name="SetStringPropertyAction.view_mode.true" />
			</Entry>
			<Entry builder="separator" />
			<Entry usedBy = "EDITOR" >
				<Entry name="code.PurgeAction" accelerator="DELETE"/>
                <Entry name="UndoAction" accelerator="control Z" />
                <Entry name="RedoAction" accelerator="control Y" />
            </Entry>
			<Entry name="CopyAction" accelerator="control C"/>
			<Entry name="CopySingleAction"  accelerator="control shift C"/>
			<Entry name="code.CopyQualifiedName"/>
			<Entry name="find">
				<Entry accelerator="control G" name="FindAction" />
				<Entry name="FindNextAction"  accelerator="control shift G" />
				<Entry name="FindPreviousAction" />
			</Entry>
			<Entry usedBy = "EDITOR" >
				<Entry name="menu_extensions">
					<Entry name="EditDetailsAction" accelerator="F3" />
					<Entry name="EditDetailsInDialogAction" accelerator="control F3" />
					<Entry name="DeleteDetailsAction" />
					<Entry builder="separator" />
					<Entry name="EditAttributesAction" accelerator="alt F9" />
					<Entry name="attributes_AddAttributeAction" />
					<Entry name="AssignAttributesAction" />
					<Entry name="CopyAttributes" />
					<Entry name="PasteAttributes" />
					<Entry name="menu_removeAttribute">
						<Entry name="attributes_RemoveFirstAttributeAction" />
						<Entry name="attributes_RemoveLastAttributeAction" />
						<Entry name="attributes_RemoveAllAttributesAction" />
					</Entry>
					<Entry name="ShowAttributeDialogAction" />
				</Entry>
			</Entry>
		</Entry>
		<Entry name="view">
			<Entry name="zoom">
				<Entry accelerator="alt UP" name="ZoomInAction" />
				<Entry accelerator="alt DOWN" name="ZoomOutAction" />
			</Entry>
			<Entry name="menu_scroll_map">
				<Entry name="FreeScrollAction.LEFT" />
				<Entry name="FreeScrollAction.UP" />
				<Entry name="FreeScrollAction.RIGHT" />
				<Entry name="FreeScrollAction.DOWN" />
				<Entry accelerator="alt W" name="MoveSelectedNodeAction.LEFT" />
				<Entry accelerator="alt C" name="MoveSelectedNodeAction.CENTER" />
				<Entry accelerator="alt E" name="MoveSelectedNodeAction.RIGHT" />
				<Entry name="MoveSelectedNodeAction.TOP" />
				<Entry name="MoveSelectedNodeAction.BOTTOM" />
				<Entry name="MoveSelectedNodeAction.TOP_LEFT" />
				<Entry name="MoveSelectedNodeAction.TOP_RIGHT" />
				<Entry name="MoveSelectedNodeAction.BOTTOM_LEFT" />
				<Entry name="MoveSelectedNodeAction.BOTTOM_RIGHT" />
			</Entry>
			<Entry name="controls">
				<Entry name="ToggleMenubarAction" builder="removeOnMac" />
				<Entry name="ToggleToolbarAction" />
				<Entry name="ShowFilterToolbarAction" accelerator="control F" />
				<Entry name="ToggleScrollbarsAction" />
				<Entry name="ToggleMapOverviewAction" />
				<Entry name="SetBooleanPropertyAction.spotlight" />
			</Entry>
			<Entry name="menu_viewmode">
				<Entry name="ShowSelectionAsRectangleAction" />
				<Entry name="show_icons" textKey="MapProperty.show_icons">
					<Entry builder="radio_button_group">
						<Entry name="SetStringMapPropertyAction.show_icons.ABOVE_NODES" />
						<Entry name="SetStringMapPropertyAction.show_icons.BESIDE_NODES" />
						<Entry name="SetStringMapPropertyAction.show_icons.HIDE" />
					</Entry>
				</Entry>
				<Entry name="SetBooleanPropertyAction.show_connectors" />
				<Entry name="SetBooleanPropertyAction.show_note_icon_in_tooltip" />
				<Entry name="SetBooleanPropertyAction.slow_scroll_selected_node" />
				<Entry name="map_overview" >
					<Entry name="SetStringPropertyAction.map_overview_bounds." />
					<Entry builder="radio_button_group" name="map_overview_attach_point" textKey="map_overview_attach_point">
						<Entry name="SetStringPropertyAction.map_overview_attach_point.SOUTH_EAST" />
						<Entry name="SetStringPropertyAction.map_overview_attach_point.SOUTH_WEST" />
						<Entry name="SetStringPropertyAction.map_overview_attach_point.NORTH_EAST" />
						<Entry name="SetStringPropertyAction.map_overview_attach_point.NORTH_WEST" />
					</Entry>
				</Entry>
			</Entry>
			<Entry name="SetBooleanPropertyAction.code_showOutsideDependencies" />
			<Entry name="menu_displayAttributes">
				<Entry builder="radio_button_group">
					<Entry name="ShowSelectedAttributesAction" />
					<Entry name="ShowAllAttributesAction" />
					<Entry name="HideAllAttributesAction" />
				</Entry>
				<Entry builder="separator" />
				<Entry name="SetBooleanMapPropertyAction.show_icon_for_attributes" />
			</Entry>
		</Entry>
		<Entry name="navigate" builder="menu_navigate">
			<Entry name="NavigationPreviousMapAction" />
			<Entry name="NavigationNextMapAction" />
			<Entry builder="separator" />
			<Entry name="SetBooleanMapViewPropertyAction.folding_follows_selection" />
			<Entry name="fold">
				<Entry name="ToggleFoldedAction" accelerator="SPACE" />
				<Entry name="ShowNextChildAction" accelerator="shift SPACE" />
				<Entry name="ToggleChildrenFoldedAction" accelerator="control SPACE" />
				<Entry name="UnfoldOneLevelAction" accelerator="alt PAGE_DOWN" />
				<Entry name="FoldOneLevelAction" accelerator="alt PAGE_UP" />
				<Entry name="UnfoldAllAction" accelerator="alt END" />
				<Entry name="FoldAllAction" accelerator="alt HOME" />
			</Entry>
			<Entry builder="separator" />
			<Entry name="SelectAllAction" accelerator="control A" />
			<Entry name="SelectBranchAction" accelerator="control shift A" />
			<Entry builder="separator" />
			<Entry name="MoveToRootAction" accelerator="ESCAPE" />
			<Entry name="GotoNodeAction" />
			<Entry name="JumpInAction"  accelerator="shift ESCAPE"/>
			<Entry name="JumpOutAction"  accelerator="control ESCAPE"/>
			<Entry name="FreeSelectNodeAction.LEFT" />
			<Entry name="FreeSelectNodeAction.UP" />
			<Entry name="FreeSelectNodeAction.RIGHT" />
			<Entry name="FreeSelectNodeAction.DOWN" />
			<Entry name="NextNodeAction.BACK" accelerator="control alt LEFT" />
			<Entry name="NextNodeAction.FORWARD" accelerator="control alt RIGHT" />
			<Entry name="NextNodeAction.BACK_N_FOLD" accelerator="control shift LEFT" />
			<Entry name="NextNodeAction.FORWARD_N_FOLD" accelerator="control shift RIGHT" />
			<Entry name="NotFoldingNextPresentationItemAction" accelerator="control alt SPACE" />
			<Entry name="NextPresentationItemAction" accelerator="control shift SPACE" />
			<Entry name="BackAction" accelerator="alt LEFT" />
			<Entry name="ForwardAction" accelerator="alt RIGHT" />
			<Entry name="links">
				<Entry name="FollowLinkAction" accelerator="control ENTER" />
				<Entry builder="menu_goto_links" />
			</Entry>
		</Entry>
		<Entry name="filter">
			<Entry name="SelectFilteredElementAction.NODE" />
			<Entry name="SelectFilteredElementAction.NODE_AND_CONNECTOR" />
			<Entry name="SelectFilteredElementAction.CONNECTOR" />
            <Entry name="ShowAncestorsAction" />
			<Entry name="ShowDescendantsAction" />
			<Entry name="HideMatchingNodesAction" />
			<Entry builder="separator" />
			<Entry name="UndoFilterAction" />
			<Entry name="RedoFilterAction" />
			<Entry name="ApplyToVisibleAction" />
			<Entry name="ReapplyFilterAction" />
			<Entry name="SelectFilteredNodesAction" />
			<Entry name="ApplySelectedViewConditionAction" />
			<Entry name="ApplyNoFilteringAction" />
			<Entry name="EditFilterAction" />
			<Entry builder="separator" />
			<Entry name="SelectSearchRootAction"/>
			<Entry name="QuickFindAction.FORWARD_VISIBLE" />
			<Entry name="QuickFindAction.FORWARD" />
			<Entry name="QuickFindAction.FORWARD_REMOVE_FILTER" />
			<Entry name="QuickFindAction.BACK_VISIBLE" />
			<Entry name="QuickFindAction.BACK" />
			<Entry name="QuickFindAction.BACK_REMOVE_FILTER" />
			<Entry builder="separator" />
			<Entry name="QuickHighlightAction" />
			<Entry name="QuickFindAllAction" />
			<Entry name="QuickFilterAction" />
            <Entry name="QuickAndFilterAction" />
            <Entry name="QuickOrFilterAction" />
		</Entry>
		<Entry name="extras">
			<Entry name="SetAcceleratorOnNextClickAction" />
			<Entry name="acceleratorPresets">
				<Entry name="load_accelerator_presets" >
					<Entry builder="acceleratorPresets" />
					<Entry builder="noActions" />
				</Entry>
				<Entry name="SaveAcceleratorPresetsAction" />
			</Entry>
			<Entry builder="separator" />
			<Entry name="OpenUserDirAction" />
			<Entry name="ShowPreferencesAction" accelerator="control COMMA" builder="removeOnMac"/>
			<Entry builder="conditionalActionBuilder" property="enableUserDefinedInterfaceStrings"
				actionKey="TextChangeHotKeyAction"
				actionClass="org.freeplane.core.ui.textchanger.TextChangeHotKeyAction"
				accelerator="control shift F10" />
		</Entry>
		<Entry name="mindmaps">
			<Entry name="modes" builder="navigate_modes,radio_button_group" />
			<Entry builder="separator" />
			<Entry name="maps">
				<Entry builder="navigate_maps,radio_button_group" />
			</Entry>
		</Entry>
		<Entry name="help" textKey="menu_help">
            <Entry name="CommandSearchAction" accelerator="control F1" />
            <Entry builder="separator" />
			<Entry name="web_resources">
				<Entry name="OpenFreeplaneSiteAction" />
                <Entry name="AskForHelp" />
				<Entry name="ReportBugAction" />
				<Entry name="RequestFeatureAction" />
			</Entry>
			<Entry name="AboutAction" />
			<Entry accelerator="F1" name="code.explorerDocumentation" />
			<Entry name="code.introductionVideo" />
			<Entry name="DocumentationAction" />
            <Entry name="FreeplaneHandbook" />
            <Entry name="HotKeyInfoAction" />
		</Entry>
	</Entry>
	<Entry name="map_popup" builder="map_popup" processOnPopup="true">
		<Entry builder="navigate_maps,radio_button_group" />
		<Entry builder="separator" />
		<Entry name="SetBooleanPropertyAction.code_showOutsideDependencies" />
		<Entry builder="separator" />
		<Entry name="ToggleMenubarAction" builder="removeOnMac" />
		<Entry name="ToggleToolbarAction" />
		<Entry name="ShowFilterToolbarAction" />
		<Entry name="ToggleScrollbarsAction" />
		<Entry name="ToggleMapOverviewAction" />
		<Entry accelerator="ESCAPE" name="MoveToRootAction" />
	</Entry>
	<Entry name="node_popup" builder="node_popup">
		<Entry name="JumpInAction"/>
		<Entry name="JumpOutAction" />
		<Entry name="menu_copy">
			<Entry name="CopyAction" accelerator="control C"/>
			<Entry name="CopySingleAction"  accelerator="control shift C"/>
			<Entry name="code.CopyQualifiedName"/>
		</Entry>
		<Entry builder="separator" />
		<Entry name="filter">
			<Entry name="SetBooleanPropertyAction.code_showOutsideDependencies" />
			<Entry name="code.FilterClassesByDependencies" />
			<Entry name="code.ShowAllClassesAction" />
			<Entry builder="separator" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.CONNECTED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.CONNECTED.MAXIMUM_RECURSION" />
			<Entry builder="separator" />
			<Entry name="code.FilterCyclesAction" />
			<Entry name="code.SelectCyclesAction" />
		</Entry>
		<Entry usedBy = "EDITOR" >
			<Entry name="edit">
				<Entry name="EditDetailsAction" accelerator="F3" />
				<Entry name="EditDetailsInDialogAction" accelerator="control F3" />
				<Entry name="DeleteDetailsAction" />
				<Entry builder="separator" />
				<Entry name="EditAttributesAction" accelerator="alt F9" />
				<Entry name="attributes_AddAttributeAction" />
				<Entry name="AssignAttributesAction" />
				<Entry name="menu_removeAttribute">
					<Entry name="attributes_RemoveFirstAttributeAction" />
					<Entry name="attributes_RemoveLastAttributeAction" />
					<Entry name="attributes_RemoveAllAttributesAction" />
				</Entry>
				<Entry name="ShowAttributeDialogAction" />
			</Entry>
		</Entry>
		<Entry builder="separator" />
		<Entry name="goto" processOnPopup="true">
			<Entry builder="link_actions" />
		</Entry>
	</Entry>
</FreeplaneUIEntries>
